decl
	int fact(int n);
enddecl

int fact(int n)
{
	decl
		int f;
	enddecl
	begin
		if(n<=1) then
			f=1;
		else
			f=n*fact(n-1);
		endif;
		return f;
	end
}

int main()
{
	decl
		int n,m,res;
	enddecl
	begin
		read(n);
		while( n >= 1 ) do
			read(m);
			res = fact(m);
			write(res);
			n = n-1;
		endwhile;
		return 0;
	end	
}